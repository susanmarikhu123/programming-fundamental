#include<stdio.h>
int main() {
int a,b,c,;
printf("Enter The Fir:\n");
scanf("%d",&a);
printf("Enter The Second Number:\n");
scanf("%d",&b);
printf("Enter The Third Number:\n");
scanf("%d",&c);
sum=a+b+c;
printf("The Sum Of Three Digits is :%d",sum);
return 0;
